													<a href="{{path('change_locale', {'locale' : locale})}}"><img style="width:5%; margin-left:90%" src="{{asset ('images/'~locale~'.png')}}" alt="{{locale}}">
													</a>
												</li>
											{% endif %}
										{% endfor %}

									</ul>#}
								</nav>
								<div class="mobile-menu"></div>
								<!-- menu end -->
							</div>
						</div>
					</div>
				</div>
			</div>
